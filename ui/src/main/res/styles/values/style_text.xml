<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ManropeSemiBold">
        <item name="fontFamily">@font/manrope_semibold</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ManropeBold">
        <item name="fontFamily">@font/manrope_bold</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ManropeExtraBold">
        <item name="fontFamily">@font/manrope_extrabold</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Body.P2" parent="ManropeSemiBold">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black01_chat</item>
    </style>

    <style name="Body.P2.Light06" parent="ManropeSemiBold">
        <item name="android:textColor">@color/light06_chat</item>
    </style>

    <style name="Body.P2.Main" parent="Body.P2">
        <item name="android:textColor">@color/text_main_chat</item>
    </style>

    <style name="Title.H6" parent="ManropeBold">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_main_chat</item>
    </style>

    <style name="Title.H6.Active" parent="Title.H6">
        <item name="android:textColor">@color/color_active_chat</item>
    </style>

    <style name="Title.H6.Light06" parent="ManropeBold">
        <item name="android:textColor">@color/light06_chat</item>
    </style>

    <style name="Title.H3" parent="ManropeExtraBold">
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">-0.04</item>
        <item name="android:textColor">@color/black01_chat</item>
    </style>

</resources>